{
    "name": "design-patterns-typescript",
    "version": "0.1.0",
    "description": "Design patterns with typescript",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/LucasMasayuki/design-patterns-typescript.git"
    },
    "keywords": [
      "Design patterns",
      "TypeScript",
      "TDD",
      "jest",
      "eslint"
    ],
    "author": "Lucas Masayuki",
    "main": "dist/main/server.js",
    "scripts": {
      "lint": "eslint src/**/*.ts",
      "format": "eslint src/**/*.ts --fix",
      "test": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage"
    },
    "dependencies": {
      "@types/jest": "^26.0.20",
      "module-alias": "^2.2.2",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^4.2.4"
    },
    "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^4.23.0",
      "@typescript-eslint/parser": "^4.23.0",
      "eslint": "^7.22.0",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-import-resolver-typescript": "^2.4.0",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-prettier": "^3.4.0",
      "husky": "^6.0.0",
      "jest": "^26.6.3",
      "prettier": "2.3.0"
    },
    "husky": {
      "hooks": {
        "pre-commit": "yarn lint"
      }
    },
    "_moduleAliases": {
      "@": "dist"
    }
  }
